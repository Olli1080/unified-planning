(define (domain robots-domain)
 (:requirements :strips :typing :negative-preconditions :equality :numeric-fluents)
 (:types Location)
 (:predicates (robot_at_0 ?p0 - Location) (robot_at_1 ?p0 - Location))
 (:functions (battery_charge_0) (battery_charge_1))
 (:action move_0
  :parameters ( ?l_from - Location ?l_to - Location)
  :precondition (and (<= 10 (battery_charge_0)) (not (= ?l_from ?l_to)) (robot_at_0 ?l_from) (not (robot_at_0 ?l_to)))
  :effect (and (not (robot_at_0 ?l_from)) (robot_at_0 ?l_to) (assign (battery_charge_0) (- (battery_charge_0) 10))))
 (:action move_1
  :parameters ( ?l_from - Location ?l_to - Location)
  :precondition (and (<= 10 (battery_charge_1)) (not (= ?l_from ?l_to)) (robot_at_1 ?l_from) (not (robot_at_1 ?l_to)))
  :effect (and (not (robot_at_1 ?l_from)) (robot_at_1 ?l_to) (assign (battery_charge_1) (- (battery_charge_1) 10))))
)
